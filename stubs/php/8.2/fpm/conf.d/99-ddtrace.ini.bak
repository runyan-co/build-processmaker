[ddtrace]
datadog.trace.analytics_enabled=1
datadog.dbm_propagation_mode='full'
datadog.phpredis_analytics_sample_rate=0.2
datadog.logs_injection=1
datadog.trace.client_ip_enabled=1
datadog.trace.spans_limit=2048
datadog.trace.remove_autoinstrumentation_orphans=1
datadog.trace.http_client_split_by_domain=1
datadog.trace_health_metrics_enabled=1
datadog.trace.128_bit_traceid_logging_enabled=1
datadog.service_mapping=laravelqueue:processmaker-queue
datadog.profiling.experimental_timeline_enabled=1
